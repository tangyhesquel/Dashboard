@using System.Web.Helpers;
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/Scripts/KendoUI/styles/kendo.default.min.css" rel="stylesheet" />
    <link href="~/Scripts/KendoUI/styles/kendo.common.min.css" rel="stylesheet" />
    <script src="~/Scripts/KendoUI/js/jquery.min.js"></script>
    <script src="~/Scripts/KendoUI/js/jszip.min.js"></script>
    <script src="~/Scripts/KendoUI/js/kendo.web.min.js"></script>
    <script src="~/Scripts/KendoUI/js/kendo.all.min.js"></script>

</head>
<body>
    <div>
        <table>
            <tr>

                <td> Begindate: <input id="begindate"></td>

                <td> Enddate: <input id="enddate"></td>
                <td><button type="button" onclick="twst()">Query</button></td>
                <td><button id="inquirybutton" onclick="twst()" style="color:blue">查询</button></td>

                <td><button id="closebutton" onclick="window.opener=null;window.close();" style="color:blue"> 关闭</button></td>@*关闭-CLOSE*@

            </tr>

        </table>
    </div>

    <script>

        $("#begindate").kendoDatePicker({
            format: "{0:yyyy-MM-dd}"
        });
        $("#enddate").kendoDatePicker({

            format: "{0:yyyy-MM-dd}"
        });
    </script>

</body>
</html>

